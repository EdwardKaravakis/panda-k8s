{
  "db": {
    "engine": "mariadb",
    "useMySQLdb": true,
    "schema": "harvester",
    "verbose": true
  },
  "master": {
    "gname": "zp",
    "harvester_id": "${HARVESTER_ID}",
    "uname": "atlpan"
  },
  "pandacon": {
    "ca_cert": false,
    "auth_type": "oidc",
    "auth_token": "${PANDA_AUTH_ID_TOKEN}",
    "auth_origin": "${PANDA_AUTH_VO}",
    "pandaURL": "${PANDA_URL_SSL}",
    "pandaURLSSL": "${PANDA_URL_SSL}",
    "verbose": true
  },
  "qconf": {
    "queueList": [
      "ALL"
    ]
  }
}